{
  "name": "lighthouse-audit",
  "version": "1.0.0",
  "description": "Lighthouse Audit",
  "main": "index.js",
  "scripts": {
    "install-deps": "npm i && mkdir temp",
    "start": "node index.js",
    "deploy": "gcloud functions deploy ceLighthouse --project cybagexl-staging --trigger-http --timeout 3600s --runtime=nodejs16 --entry-point app --memory 2048 --source . --gen2"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^5.9.0",
    "chrome-launcher": "^0.14.2",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "lighthouse": "^9.6.8",
    "puppeteer": "^20.7.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.2.0"
  }
}
